{
    "name": "extcode/tcpdf",
    "type": "typo3-cms-extension",
    "description": "Wrapper Extension for tcpdf",
    "license": [
        "GPL-2.0+"
    ],
    "keywords": [
        "TYPO3 CMS",
        "PDF"
    ],
    "authors": [
        {
            "name": "Daniel Gohlke",
            "email": "ext.tcpdf@extco.de",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/extcode/tcpdf/issues"
    },
    "require": {
        "php": ">=7.0.0 <7.3",
        "typo3/cms-core": "^8.7"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.0",
        "helmich/typo3-typoscript-lint": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Extcode\\TCPDF\\": "Classes"
        },
        "classmap": [
            "Resources/Private/Library/tcpdf"
        ]
    },
    "replace": {
        "tcpdf": "self.version"
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    },
    "scripts": {
        "post-autoload-dump": [
            "mkdir -p .Build/Web/typo3conf/ext/",
            "[ -L .Build/Web/typo3conf/ext/tcpdf ] || ln -snvf ../../../../. .Build/Web/typo3conf/ext/tcpdf"
        ]
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "tcpdf",
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "app-dir": ".Build",
            "web-dir": ".Build/public"
        }
    }
}